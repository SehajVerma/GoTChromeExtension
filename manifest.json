{
    "manifest_version": 2,

    "name": "Hide GoT Spoilers",
    "description": "Trying to save you from Game of Thrones Spoilers",
    "version": "1.2",
    "icons": {
        "96":"icon-stark.png"
    },
    "content_scripts": [
        {
            "matches": ["https://*/*","http://*/*"],
            "js": ["jquery-3.4.0.min.js","hide-spoiler.js"],
            "run_at":"document_end"
        }
    ]
}